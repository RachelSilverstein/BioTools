read_in_raw_data <- function(path, name) {
# Read in raw DMS data from TSV format.
# Parameters:
#   paths     character vector    paths to the TSV files of
#                                 DMS data. Length >= 1.
#   names     character vector    names of the genes, in the
#                                 same order as paths
# Value:
#   raw_data     data frame       data frame containing DMS data
# --------------------------------------------------------------
raw_data <- read.table(path, header = TRUE, stringsAsFactors = FALSE)
raw_data$gene <- rep(name, length(raw_data$wt_aa))
return(raw_data)
}
